<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data noupdate="1">

        <record id="sequence_campus_student" model="ir.sequence">

            <field name="name">Campus Student Sequence</field>
            <field name="code">campus.student</field>
            <field name="prefix">CS</field>
            <field name="padding">5</field>

        </record>
    </data>


    <data noupdate="1">

        <record id="sequence_campus_lecture" model="ir.sequence">

            <field name="name">Campus Lecture Sequence</field>
            <field name="code">campus.lecture</field>
            <field name="prefix">CL</field>
            <field name="padding">5</field>

        </record>
    </data>


    <data noupdate="1">

        <record id="sequence_campus_course" model="ir.sequence">

            <field name="name">Campus Course Sequence</field>
            <field name="code">campus.course</field>
            <field name="prefix">CC</field>
            <field name="padding">5</field>

        </record>
    </data>

</odoo>